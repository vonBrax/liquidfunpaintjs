{
  "name": "liquidfunpaintjs",
  "version": "1.1.0",
  "description": "Liquidfunpaint JS port",
  "main": "main.js",
  "workspaces": [
    "packages/*",
    "packages/types/*"
  ],
  "scripts": {
    "todo:compile": "tsc --project todo/tsconfig.json",
    "todo:serve": "parcel todo/index.html",
    "tsc:build": "tsc --build",
    "pretest": "rm -rf dist/ && npm run build:tsc",
    "test": "jest",
    "start": "webpack serve --mode development",
    "build": "webpack --config webpack.build.js --progress --hide-modules",
    "wp:build": "webpack",
    "wp:serve": "webpack-dev-server",
    "eslint": "eslint --ext .ts,.js .",
    "lint:fix": "npm run eslint -- --fix",
    "lint:tsc": "tsc --project src/web/tsconfig.json",
    "build:tsc": "tsc --build src/web && tsc --build src/worker",
    "lf:build": "docker build -t vonbrax/liquidfun:1.1.0 ./build/liquidfun",
    "lf:update": "docker run -v $PWD/static/liquidfun:/src vonbrax/liquidfun:1.1.0 update",
    "postlf:update": "rm -f ./src/common/liquidfun.js && mv ./static/liquidfun/liquidfun.js ./src/common/",
    "lf:shell": "docker run -v $PWD/static/liquidfun:/src -ti --rm vonbrax/liquidfun:1.1.0 shell",
    "lf:update-win": "docker run -v %cd%/static/liquidfun:/src --rm vonbrax/liquidfun:1.1.0 update",
    "lf:shell-win": "docker run -v %cd%/static/liquidfun:/src -ti --rm vonbrax/liquidfun:1.1.0 shell"
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "dependencies": {
    "gl-matrix": "^3.3.0"
  },
  "devDependencies": {
    "@types/emscripten": "^1.39.4",
    "@types/gl-matrix": "^3.2.0",
    "@typescript-eslint/eslint-plugin": "^4.10.0",
    "@typescript-eslint/parser": "^4.10.0",
    "body-parser": "^1.19.0",
    "eslint": "^7.3.1",
    "eslint-config-prettier": "^7.0.0",
    "eslint-plugin-prettier": "^3.1.4",
    "express": "^4.17.1",
    "html-webpack-plugin": "^4.3.0",
    "jest": "^26.1.0",
    "jest-puppeteer": "^4.4.0",
    "morgan": "^1.10.0",
    "prettier": "^2.0.5",
    "puppeteer": "^5.0.0",
    "ts-loader": "^8.0.12",
    "tsconfig-paths-webpack-plugin": "^3.3.0",
    "typescript": "^4.1.3",
    "webpack": "^5.10.3",
    "webpack-cli": "^4.2.0",
    "webpack-dev-server": "^3.11.0",
    "worker-loader": "^3.0.7"
  }
}
