    "start": "webpack-dev-server --mode development",
    "build": "webpack --config webpack.build.js --progress --hide-modules",
    "eslint": "eslint . --ext .ts,.js --ignore-path .gitignore",
    "lint:fix": "npm run eslint -- --fix",
    "lint:tsc": "tsc --project src/web/tsconfig.json",
    "build:tsc": "tsc --build src/web",
    "lf:build": "docker build --squash -t liquidfun ./build/em-ubuntu",
    "lf:update": "docker run -v $(PWD)/static/liquidfun:/src liquidfun update",
    "lf:update-win": "docker run -v %cd%/static/liquidfun:/src liquidfun update",
    "lf:shell": "docker run -v $(PWD)/static/liquidfun:/src -ti --rm liquidfun shell",
    "lf:shell-win": "docker run -v %cd%/static/liquidfun:/src -ti --rm liquidfun shell"
  },
